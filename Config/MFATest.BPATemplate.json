{
  "name": "MFA Test",
  "style": "Tenant",
  "Fields": [
    {
      "name": "UserMFAMethods",
      "API": "Graph",
      "URL": "https://graph.microsoft.com/v1.0/users/{{UserID}}/authentication/methods",
      "Parameters": {
        "asApp": "True"
      },
      "ExtractFields": [
        "id",
        "methodType",
        "phoneNumber",
        "email",
        "deviceTag"
      ],
      "StoreAs": "JSON",
      "UseExistingInfo": false,
      "FrontendFields": [
        {
          "name": "User",
          "value": "UserMFAMethods.userPrincipalName",
          "formatter": "string"
        },
        {
          "name": "MFA Method Type",
          "value": "UserMFAMethods.methodType",
          "formatter": "string"
        },
        {
          "name": "Phone Number",
          "value": "UserMFAMethods.phoneNumber",
          "formatter": "string"
        },
        {
          "name": "Email",
          "value": "UserMFAMethods.email",
          "formatter": "string"
        },
        {
          "name": "Device Tag",
          "value": "UserMFAMethods.deviceTag",
          "formatter": "string"
        }
      ]
    }
  ]
}
