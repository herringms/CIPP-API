{
  "name": "MFA Test",
  "style": "Table",
  "Fields": [
    {
      "name": "UserMFAMethods",
      "API": "Graph",
      "URL": "https://graph.microsoft.com/v1.0/users/{{UserID}}/authentication/methods",
      "Parameters": {
        "asApp": "True"
      },
      "ExtractFields": [
        "id",
        "methodType",
        "phoneNumber",
        "email",
        "deviceTag"
      ],
      "StoreAs": "JSON",
      "UseExistingInfo": false,
      "FrontendFields": [
        {
          "name": "Display Name",
          "value": "User.displayName",
          "formatter": "string"
        },
        {
          "name": "Email Address",
          "value": "User.mail",
          "formatter": "string"
        },
        {
          "name": "MFA Enabled",
          "value": "User.mfaEnabled",
          "formatter": "bool"
        },
        {
          "name": "SMS",
          "value": "User.mfaMethods.sms",
          "formatter": "bool"
        },
        {
          "name": "Email",
          "value": "User.mfaMethods.email",
          "formatter": "bool"
        },
        {
          "name": "Device Key",
          "value": "User.mfaMethods.deviceKey",
          "formatter": "bool"
        }
      ]
    }
  ]
}
